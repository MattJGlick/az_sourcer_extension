{
  "manifest_version": 2,

  "name": "Online Sourcing Tools",
  "description": "This extension gives useful tools for AZ FBA Sourcing",
  "version": "1.0",
  "background" : {
    "scripts": [
      "background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Calculate"
  },
  "permissions": [
    "tabs",
    "http://azsourcerbackend.herokuapp.com/*",
    "storage"
  ]
}